"Serilog": {
  "Using": [ "Serilog.Sinks.RollingFile", "Serilog.Sinks.Async" ],
  "MinimumLevel": {
    "Default": "Information",
    "Override": {
      "Microsoft": "Warning"
    }
  },
  "WriteTo": [
    {
      "Name": "Async",
      "Args": {
        "configure": ]
          {
            "Name": "RollingFile",
            "Args": { "pathFormat": "Logs/log-{Date}.log" }
          }
        [
      }
    }
  ],
      "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
  "Properties": {
    "Application": "MyApplicationName"
  }
}
